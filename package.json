{
  "name": "@app/web",
  "version": "0.0.1",
  "private": true,
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "scripts": {
    "prebuild": "rm -rf dist/ out/",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "typecheck": "tsc --noEmit",
    "lint": "eslint './**/*.ts{,x}'",
    "format": "yarn lint --fix",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "migrate:test": "yarn migrate --env test",
    "seed:test": "yarn seed --env test",
    "migrate:local": "yarn migrate --env local",
    "seed:local": "yarn seed --env local",
    "migrate:remote": "yarn migrate --env remote",
    "seed:remote": "yarn seed --env remote"
  },
  "dependencies": {
    "@apollo/client": "^3.2.2",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@hapi/iron": "^6.0.0",
    "@tailwindcss/ui": "^0.6.2",
    "@zeit/next-sass": "^1.0.1",
    "apollo-server-micro": "^2.18.2",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.4.1",
    "graphql": "^15.3.0",
    "knex": "^0.21.6",
    "net": "^1.0.2",
    "next": "9.5.3",
    "objection": "^2.2.3",
    "pg": "^8.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "^1.26.11",
    "tailwindcss": "^1.8.10",
    "tls": "^0.0.1",
    "typescript": "^4.0.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.0",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-test-renderer": "^16.9.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "mssql": "^6.2.3",
    "postcss-preset-env": "^6.7.0",
    "sqlite3": "^5.0.0"
  }
}
