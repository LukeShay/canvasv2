{
  "name": "@app/web",
  "private": true,
  "scripts": {
    "build": "next build",
    "analyze": "ANALYZE=true yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "clean": "rm -rf dist/ out/ coverage/ .next/ junit.xml",
    "dev": "next dev",
    "export": "next export",
    "format": "yarn lint --fix",
    "lint": "eslint './**/*.ts{,x}'",
    "migrate": "knex migrate:latest",
    "migrate:local": "yarn migrate --env local",
    "migrate:remote": "yarn migrate --env remote",
    "migrate:test": "yarn migrate --env test",
    "seed": "knex seed:run",
    "seed:local": "yarn seed --env local",
    "seed:remote": "yarn seed --env remote",
    "seed:test": "yarn seed --env test",
    "start": "next start",
    "pretest": "rm -rf test.sqlite3 && yarn migrate:test",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "version": "0.0.1",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
